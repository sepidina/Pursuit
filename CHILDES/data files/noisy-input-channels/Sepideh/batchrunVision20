#javac basicWL/BasicWL.java		

cp data/vision20-speech0/utterance.txt data/
cp data/vision20-speech0/scene.txt data/
for value in {1..50}
do
java basicWL.BasicWL
echo $value
done
mv results/results.txt results/results-vision20-speech0.txt

